<main class="single__main">
  <section class="single__container">
    <p>アカウントをお持ちでないかたはこちら</p>
    <%= link_to "新規会員登録", new_user_registration_path %>
    <div class="single__inner">
      <div class="single__content">
        <button id="facebook-login" class="btn-default btn-sns btn-sns-facebook"><i class="icon-facebook"></i>Facebookで登録する</button>
        <button id="google-login" class="btn-default btn-sns btn-sns-google">Googleで登録する</button>
      </div>
    </div>
  </section>
  <section>
    <form action="">
      <input type="text" placeholder="メールアドレス">
      <input type="text" placeholder="パスワード">
      <%# form_for (@user, url: "/login/new/html") do |f| %>
        <%= recaptcha_tags({class: "action" }) %>
      <%# <% end %>
      <input type="submit">
    </form>
  </section>
</main>